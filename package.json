{
  "name": "medium-to-markdown",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "convert": "pnpm exec node index.js",
    "commit": "pnpm exec cz",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "bin": {
    "medium-to-markdown": "bin/medium-to-markdown.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@mozilla/readability": "0.6.0",
    "commander": "^14.0.2",
    "jsdom": "20.0.3",
    "playwright": "1.57.0",
    "turndown": "7.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "vitest": "^4.0.17"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
